<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.muzeumfrontendjavafx.FestmenyHozzaadasView">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="20.0">
         <children>
            <Label prefWidth="75.0" text="Title" />
            <TextField fx:id="textFieldTitle" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" spacing="20.0">
         <children>
            <Label prefWidth="75.0" text="Year" />
            <Spinner fx:id="spinnerYear" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" spacing="20.0">
         <children>
            <Label prefWidth="75.0" text="On display" />
            <CheckBox fx:id="checkBoxOnDisplay" mnemonicParsing="false" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onMouseClicked="#hozzaadas" prefWidth="245.0" text="Hozzáadás" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
